{
    "compilerOptions": {
      "target": "es6",
      "module": "commonjs",
      "outDir": "./public/js",
      "strict": true,
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "forceConsistentCasingInFileNames": true
    },
    "exclude": ["node_modules", "models", "routes"],
    "include": ["./public/ts/**/*"]
}